Static Function _CriaTrab()

Local _aTmp := {}
Local _aTm2 := {}

Begin Transaction

cQry2 :=""
cQry2 := "DELETE  PA1010 "

TCSQLEXEC(cQry2)

If TCSQLExec(cQry2) <> 0
	ApMsgAlert( AllTrim(TCSQLError()), 'Contate o administrador' )
EndIf
end Transaction


AAdd ( _aTmp, {"LOG_LINLOG" , "C", 004, 00} )
AAdd ( _aTmp, {"LOG_FUNCID" , "C", 020, 00} )
AAdd ( _aTmp, {"LOG_TRAREF" , "C", 050, 00} )
