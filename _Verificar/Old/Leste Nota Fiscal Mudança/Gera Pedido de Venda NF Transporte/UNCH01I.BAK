/*/

User Function ExtractErr(aErro)

Local cMensagem := ""
Local _x

For _x := 1 to Len(aErro)
	If _x == 1
		cMensagem += AllTrim(aErro[_x])
		cMensagem := StrTran(cMensagem,CHR(13)+CHR(10),"")+". "
	Endif
	nPos := AT("< --",aErro[_x])
	If nPos > 0
		cMensagem += AllTrim(aErro[_x])
		Exit
	Endif
Next _x

Return(cMensagem)

