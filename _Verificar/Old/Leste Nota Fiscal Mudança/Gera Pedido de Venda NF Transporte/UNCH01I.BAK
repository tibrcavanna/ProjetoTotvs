Static Function ProcImp()

nTamArq := fFileSize(nHandle)
oProcess:SetRegua1(nTamArq)

While ( !Empty((cString := fReadFile(nHandle,nTamArq,0,@nBytes))) )
	
	oProcess:IncRegua1("Analisando transações no arquivo csv..")
	
	// Fim de Arquivo
	If Len(cString) < 3
		Exit
	Endif
	
	nPosic  := 0
	aLinha  := {}
	cTxtLin := cString
	While (!Empty(cTxtLin))
		If (((nPosic := AT(";",cTxtLin)) > 0))
			AAdd(aLinha,Substr(cTxtLin,1,nPosic-1))
			cTxtLin := Stuff(cTxtLin,1,nPosic,"")
		Else
			AAdd(aLinha,Substr(cTxtLin,1,Len(AllTrim(cTxtLin))))
			cTxtLin := Stuff(cTxtLin,1,Len(cTxtLin),"")
		Endif
	Enddo
	
	// Garante 24 posições
	If Len(aLinha) == 22
		AAdd(aLinha,"")
		AAdd(aLinha,"")
	ElseIf Len(aLinha) == 23
		AAdd(aLinha,"")
	Endif
	
	
	// Estrutura de aLinha:
	// [01] - Import Status
	// [02] - Import Cod
	// [03] - Import Message
	// [04]	- Empresa
	// [05] - Armazem
	// [06] - Filler1
	// [07] - Endereço
	// [08] - Filler1
	// [09] - Item
	// [10] - Codigo Produto
	// [11] - Descrição Produto
	// [12] - Data de Estoque
	// [13] - Zona Armaz.
	// [14] - Custo Total
	// [15] - Propr. local
	// [16] - Filler4
	// [17] - Estoque Disponivel
	// [18] - UM1
	// [19] - Bloquado
	// [20] - UM2
	// [21] - Alocado
	// [22] - UM3
	// [23] - Estoque por Ordm
	// [24] - UM4
	
	If Len(aLinha) < 24
		Alert("Estrutura do arquivo CSV inválida ("+AllTrim(STR(Len(aLinha)))+" posições).")
		Return
	Endif
	
	If Len(aLinha) > 0 .AND. Val(StrTran(aLinha[14],",",".")) <> 0
		
		// [05] - Armazem
		// [07] - Endereco
		// [10] - Codigo Produto
		// [11] - Descrição Produto
		// [14] - Custo Total
		// [17] - Estoque Disponivel
		
		
		cArmazem  := aLinha[05]
		cEndereco := aLinha[07]
		cProduto  := aLinha[09]
		cDesProd  := Upper(aLinha[11])
		nCusto    := Val(StrTran(aLinha[14],",","."))
		nQtdEst   := Val(StrTran(aLinha[17],",",".")) // Valor da Transação
		
		
		//	cCcusto := PadR(StrTran(aLinha[32],".",""),9) // C.Custo
		
		lTudoOk := .T.
		
		//If nCusto == 0
		//	InputLog(cFuncId,cTraRef,nTraVlr,"Valor não informado.")
		//lTudoOk := .F.
		//	 Endif
		If lTudoOk
			InputLin(cEndereco,cProduto,cDesProd,nCusto,nQtdEst,cArquivo)
		Endif
		
	Endif
	
Enddo

DbSelectArea("LOG")
If LOG->(RecCount()) > 0
	MsgInfo("Foram encontradas inconsistências no arquivo 'csv'.")
	u_ReportLog(cArquivo)
	Return
Endif

u_ReportLog(cArquivo)

Return()



