			aadd(aItens,{"C6_TES"			,"617" 				,Nil})
		  //	aadd(aItens,{"C6_LOCAL"			,PA1->PA1_LOCPAD	,Nil})
			//aadd(aItens,{"C6_ENTREG"		,dDataBase						,Nil})
		  //	aadd(aItens,{"C6_DESCRI"		,PA1->PA1_DESC					,Nil})
			
