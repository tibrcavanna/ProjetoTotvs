	ROLLBACKSX8()
	
	WHILE SC5->C5_NUM > cPEDIDO
		WHILE (GETSX8LEN() > nSAVESX8)
			CONFIRMSX8()
		ENDDO
		cPEDIDO := GETSXENUM("SC5","C5_NUM")
	ENDDO
	
