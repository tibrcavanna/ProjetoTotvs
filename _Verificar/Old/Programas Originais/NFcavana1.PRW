Static Function CLASFIS()

Local nLen := Len(xCLFISCAL)

If nLen > 1
 @ 14, 23   PSAY Transform(xCLAS_FIS[2],"@r 9999.99.99") 
EndIf

If nLen > 2
 @ 14, 45   PSAY Transform(xCLAS_FIS[3],"@r 9999.99.99") 
EndIf

Return

Static Function GERAMSG()

_xMsgGeral := ""

If !Empty(_xTESMSG)
  For _n := 1 to len(_xTESMSG)
    _xMsgGeral := _xMsgGeral+iif(!empty(_xMsgGeral)," / ","")+alltrim(FORMULA(_xTESMSG[_n])) //msg padrao do TES
  Next _n
EndIf
If !Empty(xCOD_MENS)
   _xMsgGeral := _xMsgGeral+iif(!empty(_xMsgGeral)," / ","")+alltrim(FORMULA(xCOD_MENS)) //msg padrao do pedido
Endif
If !Empty(xMENSAGEM)
   _xMsgGeral := _xMsgGeral+iif(!empty(_xMsgGeral)," / ","")+ALLTRIM(xMENSAGEM) //msg padrao do pedido
Endif

Return

