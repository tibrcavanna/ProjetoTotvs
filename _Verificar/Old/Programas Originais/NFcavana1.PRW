@ 43, 002 PSAY xVOLUME  Picture "@E@Z 999,999.99"             // Quant. Volumes
@ 43, 022 PSAY xESPECIE Picture "@!"                          // Especie
@ 43, 041 PSAY " "                                           // Res para Marca
