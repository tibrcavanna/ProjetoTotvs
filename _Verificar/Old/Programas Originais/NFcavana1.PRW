@ 39, 084 PSAY xPLCVEIC                             // Res. p/Placa do Veiculo
@ 39, 098 PSAY " "                                  // Res. p/xEST_TRANSP                          // U.F.

If !EMPTY(xCGC_TRANSP)                              // Se C.G.C. Transportador nao for Vazio
   @ 39, 105 PSAY xCGC_TRANSP Picture "@R 99.999.999/9999-99"
Else
   @ 39, 105 PSAY " "                               // Caso seja vazio
