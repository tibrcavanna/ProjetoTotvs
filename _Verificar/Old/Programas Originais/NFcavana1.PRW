Static Function IMPDET()

Local I             := 1
Local _cDesc   := ""
Local _nTotLin  := 0
Local _nResto  := 0
Local _nInic      := 1
Local _cClFis   := ""

nTamDet :=10            // Tamanho da Area de Detalhe

For I:= 1 to nTamDet

      If I<= Len(xCOD_PRO)

          _cDesc    := alltrim(MSMM(posicione("SB1",1,xfilial("SB1")+xCOD_PRO[I],"B1_DESC_P")))
          _cClFis   := posicione("SB1",1,xfilial("SB1")+xCOD_PRO[I],"B1_POSIPI")
          
          @ nLin, 000  PSAY xCOD_PRO[I]
          @ nLin, 017  PSAY substr(xDESCRICAO[I],1,67-LEN(ALLTRIM(xDESCPES[I])))+" "+alltrim(xDESCPES[I])

          If !empty(_cDesc)
		         For _nX := 1 to MlCount(AllTrim(_cDesc),67)
                  nLin ++
                  @ nLin, 017  PSAY MemoLine(AllTrim(_cDesc),67,_nX)
		         Next _nX
          EndIf
     
          @ nLin, 086  PSAY Transform(_cClFis,"@r 9999.99.99") //xCOD_FIS[I]
          @ nLin, 097  PSAY xCOD_TRIB[I]
          @ nLin, 103  PSAY xUNID_PRO[I]
          @ nLin, 105  PSAY ROUND(xQTD_PRO[I],4)      Picture "@E 99,999.9999"
          @ nLin, 127  PSAY round(xPRE_UNI[I],2)     Picture "@E 99,999,999.99"
          @ nLin, 155  PSAY xVAL_MERC[I]              Picture "@E 999,999,999.99"
          @ nLin, 172  PSAY xICM_PROD[I]              Picture "99.99"
          @ nLin, 180  PSAY xIPI[I]                   Picture "99.99"
          @ nLin, 187  PSAY xVAL_IPI[I]               Picture "@E 999,999,999.99"

      Endif

      nLin ++

Next

Return


