/*/
Static Function Imprime()

//+--------------------------------------------------------------+
//¦                                                              ¦
//¦              IMPRESSAO DA N.F. DA Nfiscal                    ¦
//¦                                                              ¦
//+--------------------------------------------------------------+

//+-------------------------------------+
//¦ Impressao do Cabecalho da N.F.      ¦
//+-------------------------------------+

@ 00, 000 PSAY Chr(15)                // Compressao de Impressao

@ 00, 185 PSAY CHR(14)+xNUM_NF+CHR(15)           // Numero da Nota Fiscal

nTamObs  := 64
_nLinhas := MlCount(AllTrim(_xMsgGeral),nTamObs)

If _nlinhas <> 0
 @ 01, 00 PSAY MemoLine(AllTrim(_xMsgGeral),nTamObs,1)
EndIf

If _nlinhas > 1
 @ 02, 00 PSAY MemoLine(AllTrim(_xMsgGeral),nTamObs,2)
EndIf

If mv_par04 == 1 //entrada
   @ 02, 151 PSAY "X"
Else
   @ 02, 168 PSAY "X"
Endif

If _nlinhas > 2
 @ 03, 00 PSAY MemoLine(AllTrim(_xMsgGeral),nTamObs,3)
EndIf
If _nlinhas > 3
 @ 04, 00 PSAY MemoLine(AllTrim(_xMsgGeral),nTamObs,4)
EndIf
If _nlinhas > 4
 @ 05, 00 PSAY MemoLine(AllTrim(_xMsgGeral),nTamObs,5)
EndIf
If _nlinhas > 5
 @ 06, 00 PSAY MemoLine(AllTrim(_xMsgGeral),nTamObs,6)
EndIf

@ 06, 069 PSAY substr(xNATUREZA,1,34)               // Texto da Natureza de Operacao

If mv_par04 == 1
    @ 06, 104 PSAY xCF[1] Picture PESQPICT("SD1","D1_CF") // Codigo da Natureza de Operacao
Else
    _xCF := ""
    For _n := 1 to len(xCF)
      _xCF += alltrim(xCF[_n])+iif(len(xCF) > 1,"/","")
    Next _n
    @ 06, 104 PSAY alltrim(_xCF) Picture PESQPICT("SD2","D2_CF") // Codigo da Natureza de Operacao
EndIf


If _nlinhas > 6
 @ 07, 00 PSAY MemoLine(AllTrim(_xMsgGeral),nTamObs,7)
EndIf
If _nlinhas > 7
 @ 08, 00 PSAY MemoLine(AllTrim(_xMsgGeral),nTamObs,8)
EndIf
If _nlinhas > 8
 @ 09, 00 PSAY MemoLine(AllTrim(_xMsgGeral),nTamObs,9)
EndIf

//+-------------------------------------+
//¦ Impressao dos Dados do Cliente      ¦
//+-------------------------------------+

@ 09, 069 PSAY substr(xNOME_CLI,1,77)              //Nome do Cliente

If !EMPTY(xCGC_CLI)                   // Se o C.G.C. do Cli/Forn nao for Vazio
   @ 09, 150 PSAY xCGC_CLI Picture "@R 99.999.999/9999-99"
Else
   @ 09, 150 PSAY " "                // Caso seja vazio
Endif                                                 

@ 09, 188 PSAY xEMISSAO              // Data da Emissao do Documento

If _nlinhas > 93
 @ 10, 00 PSAY MemoLine(AllTrim(_xMsgGeral),nTamObs,10)
EndIf

If Len(xNUM_NFDV) > 0  .and. !Empty(xNUM_NFDV[1])
   @ 11, 000 PSAY "Nota Fiscal Original No." + "  " + xNUM_NFDV[1] + "  " + xPREF_DV[1]
Endif

@ 11, 069 PSAY substr(xEND_CLI,1,65)                                 // Endereco
@ 11, 136 PSAY substr(xBAIRRO,1,25)                                  // Bairro
@ 11, 163 PSAY xCEP_CLI Picture"@R 99999-999"           // CEP
@ 11, 188 PSAY " "                                      // Reservado  p/Data Saida/Entrada

If !Empty(xSuframa)
   @ 12,000 PSAY "SUFRAMA : "+xSuframa
EndIf

@ 13, 069 PSAY substr(xMUN_CLI,1,39)                               // Municipio
@ 13, 110 PSAY xTEL_CLI                               // Telefone/FAX
@ 13, 140 PSAY xEST_CLI                               // U.F.
@ 13, 150 PSAY xINSC_CLI                              // Insc. Estadual
@ 13, 188 PSAY " "                                    // Reservado p/Hora da Saida

If mv_par04 == 2
   //Clasfis()               // Impressao de Classif. Fiscal
   @ 17, 23 PSAY xPed_Cli[1]  //numero do pedido do cliente
   @ 17, 47 PSAY cPedAtu      //numero do pedido no sistema

   //+-------------------------------------+
   //¦ Impressao da Fatura/Duplicata       ¦
   //+-------------------------------------+
   nLin:=17
   BB:=1
   nCol := 70             //  duplicatas
   DUPLIC()
Else

   @ 17, 23 PSAY xPedido[1]  //numero do pedido do cliente
Endif

//+-------------------------------------+
//¦ Dados dos Produtos Vendidos         ¦
//+-------------------------------------+

nLin := 21
ImpDet()                 // Detalhe da NF


//+-------------------------------------+
//¦ Calculo dos Impostos                ¦
//+-------------------------------------+

@ 34, 011  PSAY xBASE_ICMS  Picture"@E@Z 999,999,999.99"  // Base do ICMS
@ 34, 037  PSAY xVALOR_ICMS Picture"@E@Z 999,999,999.99"  // Valor do ICMS
@ 34, 063  PSAY xBSICMRET   Picture"@E@Z 999,999,999.99"  // Base ICMS Ret.
@ 34, 090  PSAY xICMS_RET   Picture"@E@Z 999,999,999.99"  // Valor  ICMS Ret.
@ 34, 121  PSAY xVALOR_MERC Picture"@E@Z 999,999,999.99"  // Valor Tot. Prod.

@ 36, 011  PSAY xFRETE      Picture"@E@Z 999,999,999.99"  // Valor do Frete
@ 36, 037  PSAY xSEGURO     Picture"@E@Z 999,999,999.99"  // Valor Seguro
@ 36, 063  PSAY xDESPESA    Picture"@E@Z 999,999,999.99"  // Valor Despesas
@ 36, 090  PSAY xVALOR_IPI  Picture"@E@Z 999,999,999.99"  // Valor do IPI
@ 36, 121  PSAY xTOT_FAT    Picture"@E@Z 999,999,999.99"  // Valor Total NF


   //+------------------------------------+
   //¦ Impressao Dados da Transportadora  ¦
   //+------------------------------------+

@ 39, 000  PSAY substr(xNOME_TRANSP,1,55)                       // Nome da Transport.

If xTPFRETE=='C'                                   // Frete por conta do
   @ 39, 075 PSAY "1"                              // Emitente (1)
