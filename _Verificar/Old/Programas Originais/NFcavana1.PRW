@ 47, 170 PSAY chr(14)+xNUM_NF+chr(15)                   // Numero da Nota Fiscal

@ 51, 000 PSAY chr(18)                   // Descompressao de Impressao

SetPrc(0,0)                              // (Zera o Formulario)

