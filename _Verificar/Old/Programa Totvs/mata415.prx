Function A415F4Stoq(a,b,c)

Local aArea	   := GetArea()
Local cProduto := ""
Local cFilEnt  := Nil
Local cCampo   := AllTrim(StrTran(ReadVar(),"M->",""))
Local nPosPrd  := 0

If ( SubStr(cCampo,1,2) $ "CL" .And. cCampo <> "CL_PRODUTO" )
	nPosPrd  := aScan(aHeader,{|x| AllTrim(x[2]) == "CL_PRODUTO" })
	If ( nPosPrd > 0 )
		cProduto := aCols[n,nPosPrd]
	EndIf
