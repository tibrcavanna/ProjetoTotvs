@ nGetLin,aPosGet[1,4] SAY oSay PROMPT 0    					SIZE 040,009 PICTURE PesqPict("SCK","CK_VALOR",14) PIXEL OF oDlg
oSay:Cargo := "Desconto"
@ nGetLin,aPosGet[1,6] SAY oSay PROMPT 0                     SIZE 040,009 PICTURE PesqPict("SCK","CK_VALOR",16) PIXEL OF oDlg
oSay:Cargo := "Total"
A415Total(oDlg)
