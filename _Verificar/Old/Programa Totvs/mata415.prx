	aAdd(aCpoVirtual, "CK_OPER")
EndIf	

FillGetDb(2, "SCK", "TMP1",,1,cSeek, { || &cWhile },{{bCond,bAction1,bAction2}},aNoFields,,,cQuery,,,lInclui,aCpoNCopia,aCpoVirtual)
