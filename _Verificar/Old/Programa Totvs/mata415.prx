Function MaCanDelOrc(cAliasSCJ,cAliasSCK)
Local lRetorno := .T.
If ( cAliasSCK <> Nil )
	lRetorno := Empty((cAliasSCK)->CK_NUMOP)
	If ( (cAliasSCJ)->CJ_STATUS == "B" .And. lRetorno)
		If !Empty((cAliasSCK)->CK_NUMPV)
			dbSelectArea("SC5")
			dbSetOrder(1)
			If MsSeek(xFilial("SC5")+(cAliasSCK)->CK_NUMPV)
				lRetorno := .F.	
			EndIf
		EndIf
	EndIf
EndIf

Return(lRetorno)
