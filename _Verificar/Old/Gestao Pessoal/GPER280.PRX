	   		If aValBenef[nCntP,5] > 0
				RecLock( "SEF" , .T. )
				SEF->EF_FILIAL  := cFilGravar
				SEF->EF_NUM     := cNumCheq
				SEF->EF_TIPO    := cTipo
				SEF->EF_BANCO   := cBanco
