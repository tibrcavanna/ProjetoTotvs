User Function LinhaOk()

xValDig:=0
For _n := 1 to Len(aCols)
	If !ACOLS[_n,nUsado+1]
		xValDig+=ACOLS[_n,_nValor]
	Else
		xValDig-=ACOLS[_n,_nValor]
	EndIf
Next _n

If !ACOLS[N,nUsado+1]
	if U_ExistNat()
		Return(.T.)
	Else
		Return(.F.)
	EndIf
Else
	Return(.T.)
EndIf

