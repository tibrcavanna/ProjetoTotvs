//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
// 3,4 Permitem alterar getdados e incluir linhas
// 6 So permite alterar getdados e nao incluir linhas
// Qualquer outro numero so visualiza
// AFR_tarefa cadastro de projetos - AF8_PROJET

nOpcx := 3
_nProp := 0
_nUm   := 0
_nNorm := 0
_nEspe := 0
_sAlias:= Alias()
_sRec  := Recno()

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Montando aHeader                                             ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
dbSelectArea("Sx3")
dbSetOrder(1)
dbSeek("SE2")
//.OR. AllTrim(X3_CAMPO)=="E2_TAREFA"  ;
nUsado:=0
aHeader:={}
While !Eof() .And. (x3_arquivo == "SE2")
	If AllTrim(X3_CAMPO)=="E2_XOP" .OR. AllTrim(X3_CAMPO)=="E2_VALOR";
		.OR. AllTrim(X3_CAMPO)=="E2_NATUREZ"
		IF X3USO(x3_usado) .AND. cNivel >= x3_nivel
			nUsado:=nUsado+1
			cNome := AllTrim(X3_CAMPO)
			AADD(aHeader,{ TRIM(x3_titulo), AllTrim(x3_campo), x3_picture,;
			x3_tamanho, x3_decimal,x3_vlduser,x3_usado, x3_tipo, x3_arquivo, x3_context } )
		Endif
	EndIf
	dbSkip()
EndDo

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Montando aCols                                               ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
aCols:=Array(1,nUsado+1)
dbSelectArea("Sx3")
dbSeek("SE2")
nUsado:=0
While !Eof() .And. (x3_arquivo == "SE2")
	If AllTrim(X3_CAMPO)=="E2_XOP" .OR. AllTrim(X3_CAMPO)=="E2_VALOR";
		.OR. AllTrim(X3_CAMPO)=="E2_NATUREZ" ;
		//       .OR. AllTrim(X3_CAMPO)=="E2_XPROJET"
		IF X3USO(x3_usado) .AND. cNivel >= x3_nivel
			IF nOpcx == 3
				nUsado:=nUsado+1
				IF x3_tipo == "C"
					aCOLS[1][nUsado] := SPACE(x3_tamanho)
				Elseif x3_tipo == "N"
					aCOLS[1][nUsado] := 0
				Elseif x3_tipo == "D"
					aCOLS[1][nUsado] := dDataBase
				Elseif x3_tipo == "M"
					aCOLS[1][nUsado] := ""
				Else
					aCOLS[1][nUsado] := .F.
				Endif
			Endif
		Endif
	EndIf
	dbSkip()
End
aCOLS[1][nUsado+1] := .F.

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
