	MSGBOX("Codigo NAO Existente.","Fornecedor")
	Return(.F.)
Else
	xe2_nomfor := SA2->a2_nome
	xe2_loja   := SA2->A2_LOJA
	dbselectarea("SE2")
	dbgotop()
	dbsetorder(6)
	if dbseek(xfilial()+xe2_fornece+xe2_loja+"UNI"+wnum_op)
		MSGBOX("Ja existe, por favor tente outro fornecedor.","Duplicata")
		Return(.F.)
	endif
EndIf

Return(.T.)

