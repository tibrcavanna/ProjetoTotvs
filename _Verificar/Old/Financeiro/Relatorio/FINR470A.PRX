Static Function TxMoeda( cAliasSE5, nMoedaBco )

Local aArea	:= GetArea()     
Local nTaxa	:= 0                
Local cNum	:= (cAliasSE5)->E5_NUMCHEQ

If !Empty(cNum)
	SE5->(dbSetOrder(10))
	SE5->(MsSeek(xFilial("SE5")+cNum))
	If (cAliasSE5)->( FieldPos("E5_TXMOEDA") > 0) .And. !Empty(SE5->E5_TXMOEDA)
		nTaxa := SE5->E5_TXMOEDA  
	Else
		nTaxa := RecMoeda((cAliasSE5)->E5_DTDISPO,mv_par06)
	EndIf
Else
	If (cAliasSE5)->( FieldPos("E5_TXMOEDA") > 0) .And. !Empty((cAliasSE5)->E5_TXMOEDA) .And. Val((cAliasSE5)->E5_MOEDA) == nMoedaBco
		nTaxa := (cAliasSE5)->E5_TXMOEDA  	
	Else
		nTaxa := RecMoeda((cAliasSE5)->E5_DTDISPO,mv_par06)		
	EndIf	
EndIf	   	

RestArea( aArea )

Return( nTaxa )


