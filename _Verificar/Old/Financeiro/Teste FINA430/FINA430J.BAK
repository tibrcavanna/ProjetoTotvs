				// Multa					- 10
				// Fornecedor			- 11
				// Ocorrencia			- 12
				// CGC					- 13
				// nCM					- 14
				// Rejeicao				- 15
				// Linha Inteira		- 16
	         // Autenticacao      - 17
	         // Banco             - 18	         
	         // Agencia           - 19
	         // Conta             - 20	         
				aValores := ( { cNumTit, dBaixa, cTipo,;
									 cNsNum, nDespes, nDescont,;
									 nAbatim, nValPgto, nJuros,;
									 nMulta, cForne, cOcorr,;
	                         cCGC, nCM,cRejeicao,xBuffer,;
	                         cAutentica,cBanco,cAgencia,cConta })
	
				ExecBlock("F430VAR",.F.,.F.,{aValores})
	
			Endif
	
			If Empty(cNumTit)
